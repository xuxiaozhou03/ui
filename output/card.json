{
  "wxml": [
    {
      "tag": "div",
      "attrs": {
        "class": "custom-class van-card"
      },
      "children": [
        {
          "tag": "div",
          "attrs": {
            ":class": "utils.bem('card__header', { center: centered })"
          },
          "children": [
            {
              "tag": "div",
              "attrs": {
                "class": "van-card__thumb",
                "@tap": "onClickThumb"
              },
              "children": [
                {
                  "tag": "image",
                  "attrs": {
                    "v-if": "thumb",
                    ":src": "thumb",
                    ":mode": "thumbMode",
                    ":lazy-load": "lazyLoad",
                    "class": "van-card__img thumb-class"
                  }
                },
                {
                  "tag": "slot",
                  "attrs": {
                    "v-else": "",
                    "name": "thumb"
                  }
                },
                {
                  "tag": "van-tag",
                  "attrs": {
                    "v-if": "tag",
                    "mark": "",
                    "type": "danger",
                    "custom-class": "van-card__tag"
                  }
                },
                {
                  "tag": "slot",
                  "attrs": {
                    "v-else": "",
                    "name": "tag"
                  }
                }
              ]
            },
            {
              "tag": "div",
              "attrs": {
                "class": "van-card__content {{ utils.bem('card__content', { center: centered }) }}"
              },
              "children": [
                {
                  "tag": "div",
                  "children": [
                    {
                      "tag": "div",
                      "attrs": {
                        "v-if": "title",
                        "class": "van-card__title title-class"
                      }
                    },
                    {
                      "tag": "slot",
                      "attrs": {
                        "v-else": "",
                        "name": "title"
                      }
                    },
                    {
                      "tag": "div",
                      "attrs": {
                        "v-if": "desc",
                        "class": "van-card__desc desc-class"
                      }
                    },
                    {
                      "tag": "slot",
                      "attrs": {
                        "v-else": "",
                        "name": "desc"
                      }
                    },
                    {
                      "tag": "slot",
                      "attrs": {
                        "name": "tags"
                      }
                    }
                  ]
                },
                {
                  "tag": "div",
                  "attrs": {
                    "class": "van-card__bottom"
                  },
                  "children": [
                    {
                      "tag": "slot",
                      "attrs": {
                        "name": "price-top"
                      }
                    },
                    {
                      "tag": "div",
                      "attrs": {
                        "v-if": "price || price === 0",
                        "class": "van-card__price price-class"
                      },
                      "children": [
                        {
                          "tag": "text"
                        },
                        {
                          "tag": "text",
                          "attrs": {
                            "class": "van-card__price-integer"
                          }
                        },
                        {
                          "tag": "text",
                          "attrs": {
                            "class": "van-card__price-decimal"
                          }
                        }
                      ]
                    },
                    {
                      "tag": "slot",
                      "attrs": {
                        "v-else": "",
                        "name": "price"
                      }
                    },
                    {
                      "tag": "div",
                      "attrs": {
                        "v-if": "originPrice || originPrice === 0",
                        "class": "van-card__origin-price origin-price-class"
                      }
                    },
                    {
                      "tag": "slot",
                      "attrs": {
                        "v-else": "",
                        "name": "origin-price"
                      }
                    },
                    {
                      "tag": "div",
                      "attrs": {
                        "v-if": "num",
                        "class": "van-card__num num-class"
                      }
                    },
                    {
                      "tag": "slot",
                      "attrs": {
                        "v-else": "",
                        "name": "num"
                      }
                    },
                    {
                      "tag": "slot",
                      "attrs": {
                        "name": "bottom"
                      }
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "tag": "div",
          "attrs": {
            "class": "van-card__footer"
          },
          "children": [
            {
              "tag": "slot",
              "attrs": {
                "name": "footer"
              }
            }
          ]
        }
      ]
    }
  ],
